<?xml version="1.0" ?>
<wordrad_cfg>
    <title>Beer List</title>
    <font_dir path="{{P}}" />
    <textr_dir path="{{P}}" />
    <img_dir path="{{P}}" />
    <textr_fg source="g_dd924e8a86.jpg" />
    <textr_bg source="g_f473920e94.jpg" />
    <light val="textr_fg" />
    <altw val="AC    BFSBxax" />
    <text_width val="0" />
    <font_swf source="Xcelsion.swf" />
    <font_swf source="Xcelsion_A0.swf" />
    <text_scale val="2.667000" scale0="1.333333" max_sz="36.010000" />
    <textr_fgn source="fgn_b6a58ce3b.jpg" color="#8b008b" />
    <textr_fgn source="g_f473920e94.jpg" />
    <htmlText><![CDATA[
    <font face="Xcelsion"><font size="36">The Beer List<br><br>
    {% regroup object_list by initial as initial_list %}
    {% for initial in initial_list %}
       <font size="30">{{ initial.grouper }}</font><br>
       <textformat  leftmargin="50">
       {% for beer in initial.list %}
       <font size="20" color="#8b008b"><li><a href="{{ beer.slug }}~{{theme}}.xml">{{ beer.name }}</a></li>
       </font>{% endfor %}
       </textformat>
    {% endfor %}]]>
    </htmlText>
</wordrad_cfg>
